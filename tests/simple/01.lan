fibonacci(n :: Int) : Int {
  if (n < 1)
    return n;
  if (n == 1)
    return n;
  return fibonacci(n-1)[0] + fibonacci(n-2)[0];
}

data Ponto {
  x :: Int;
  y :: Int;
}

